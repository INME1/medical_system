{
  "Name": "OrthancServer",
  "StorageDirectory": "/var/lib/orthanc/db",
  "DefaultEncoding": "UTF-8",
  "DicomAet": "ORTHANC",
  "DicomPort": 4242,
  "HttpPort": 8042,
  "HttpServerEnabled": true,
  "AuthenticationEnabled": false,

  "HttpCorsEnabled": true,
  "HttpCorsAllowOrigin": "http://35.225.63.41:3001",
  "HttpCorsAllowMethods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
  "HttpCorsAllowHeaders": ["Authorization", "Content-Type", "Accept", "Origin"],
  "HttpCorsAllowCredentials": true,

  "DicomWeb": {
    "Enable": true,
    "Root": "/dicom-web/",
    "EnableWado": true,
    "EnableQido": true,
    "EnableStow": true
  }
}
